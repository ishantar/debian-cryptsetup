# Run cryptdisks at startup ?
CRYPTDISKS_ENABLE=Yes

# Default precheck script
# see /usr/share/cryptsetup/checks
CRYPTDISKS_PRECHECK=

# Default check script
# see /usr/share/cryptsetup/checks
CRYPTDISKS_CHECK=ext2

# Default number of times to retry the password
# input, in the case that the check fails.
CRYPTDISKS_RETRY=3

# Default timeout in seconds for password prompt
CRYPTDISKS_TIMEOUT=180
